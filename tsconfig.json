{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 18",
  "_version": "2.0.0",
  "compilerOptions": {
    "lib": [
      "es2023",
      "DOM"
    ],
    "module": "node16",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node"
  }
}
